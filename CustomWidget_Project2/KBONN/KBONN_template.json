{
	"name": "Hello Konstantin",
	"description": "Custom Widget Template",
	"eula": "",
	"vendor": "Konstantin Bonn",
	"license": "",
	"id": "com.sap.sample.template_kb.0",
	"newInstancePrefix": "WidgetTemplate",
	"version": "0.0.1",
	"icon": "data:image/png;base64,",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "com-sap-sample-hw",
			"url": "https://imsac2020.github.io/github-upload/CustomWidget_Project2/KBONN/webcomponent.js",
			"integrity": "",
			"ignoreIntegrity": true
		},
		{
			"kind": "styling",
			"tag": "com-sap-sample-hw-aps",
			"url": "https://imsac2020.github.io/github-upload/CustomWidget_Project2/KBONN/aps_webcomponent.js",
			"integrity": "",
			"ignoreIntegrity": true
		}
    ],
	"properties": {
		"headingType": {
			"type": "string",
			"description": "The heading level of the widget; h1-h3. Default is h1.",
			"default" : "h1"
		},
		"widgetText": {
		"type": "string",
		"description": "The text to be displayed in the widget",
		"default" : "Hello World"
		}
	},
	"methods": {
		"setH1": {
			"description": "Update the hX tag of the KPI field to h1",
			"body": "this.headingType = 'h1';"
		},
		"setH2": {
			"description": "Update the hX tag of the KPI field to h2",
			"body": "this.headingType = 'h2';"
		},
		"setH3": {
			"description": "Update the hX tag of the KPI field to h3",
			"body": "this.headingType = 'h3';"
		},
		"getH": {
			"description": "Get the hX tag of the KPI field",
			"body": "return this.headingType;",
			"returnType": "string"
		},
		"setText": {
			"description": "Set the text value of the widget",
			"parameters": [
				{
					"name": "newTextValue",
					"type": "string",
					"description": "The displayed angle of the gauge"
				}
			],
			"body": "this.widgetText = newTextValue;"
		}
	},
	"events": {
	}
}